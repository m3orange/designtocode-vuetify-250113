{
  "version": 2,
  "name": "vuetifyjs.com",
  "alias": "vuetifyjs.com",
  "builds": [
    { "src": "packages/docs/package.json", "use": "@now/static-build" },
    { "src": "packages/docs/server.js", "use": "@now/node" }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": ".*", "dest": "/packages/docs/server.js" }
  ],
  "github": {
    "autoAlias": false,
    "silent": true
  }
}
