{
  "extends": "../../tsconfig.json",
  "include": [
    "src",
    "dev",
  ],
  "exclude": ["types-temp", "**/*.spec.cy.ts", "**/*.spec.cy.tsx"],
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "./types-temp",
    "paths": {
      "@/*": ["src/*"],
    },
    "types": [
      "node",
      "vue-router"
    ],
    "stripInternal": true,
    "skipLibCheck": false
  }
}
