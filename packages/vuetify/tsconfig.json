{
  "extends": "../../tsconfig.json",
  "include": [
    "src",
    "dev",
    "test"
  ],
  "exclude": ["types-temp", "**/*.spec.cy.ts", "**/*.spec.cy.tsx"],
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "./types-temp",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@test": ["test/index.ts"]
    },
    "types": [
      "node",
      "vue-router",
      "@vitest/browser/matchers"
    ],
    "stripInternal": true,
    "skipLibCheck": false
  }
}
